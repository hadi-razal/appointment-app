{
  "expo": {
    "name": "Medi Care",
    "slug": "medi-care",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/online-appointment.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1e3a8a"
    },
    "ios": {
     },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/online2we.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hadirasal.medicare"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/online-appointment.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-splash-screen",
      "expo-updates",
      "expo-av",
      "expo-image-picker"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "edb93d48-e625-4411-9d06-f092bd4dc42e"
      }
    },
    "owner": "hadirasal"
  }
}
